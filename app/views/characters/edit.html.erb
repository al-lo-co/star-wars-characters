<div> 

<%= form_for @character do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :gender %>
    <%= f.text_field :gender %>
  </div>
  <div>
    <%= f.label :birth_year %>
    <%= f.text_field :birth_year %>
  </div>
  <div>
    <%= f.label :height %>
    <%= f.text_field :height %>
  </div>
  <div>
    <%= f.label :mass %>
    <%= f.text_field :mass %>
  </div>

  <%= f.submit %>
<% end %>

</div>